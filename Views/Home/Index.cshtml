 @model List<Imagenes>

@{
    ViewData["Title"] = "Imagenes";
    var count = 1;
}

<div class="row">
    <img src="https://i.pinimg.com/originals/51/f6/fb/51f6fb256629fc755b8870c801092942.png" height="200" width="200"><h1>PERU FAILS</h1>
</div>

@if (Model.Count == 0) {
    <tr>
        <td colspan="9" class="text-center">No se han encontrado registros. <br>Ingrese registros porfavor.</td>
    </tr>
}
@foreach (var contacto in Model) 
{
    <tr><br>
        <td>
            @count
        </td><br>
        <td>
            Subido por: @contacto.nombreUsuario
        </td><br>
        <td>
            <img src="@contacto.imagen" alt="" height="600" width="600">
        </td><br>
        <th>
            Fecha: @contacto.date
        </th>
    </tr>
    count++;
}
       
